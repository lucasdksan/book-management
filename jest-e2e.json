{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts"
    ],
    "rootDir": ".",
    "testEnvironment": "node",
    "testRegex": "tests/.*\\.e2e-spec\\.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
        "^src/(.*)$": "<rootDir>/src/$1"
    },
    "setupFilesAfterEnv": [
        "<rootDir>/tests/setup-e2e.ts"
    ]
}